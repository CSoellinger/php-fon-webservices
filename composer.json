{
    "name": "csoellinger/php-fon-webservices",
    "description": "PHP library to handle FinanzOnline webservices.",
    "license": "Unlicense",
    "type": "library",
    "keywords": [
        "Bmf",
        "Fon",
        "FinanzOnline",
        "Webservice",
        "Session",
        "Login",
        "Logout",
        "Uid",
        "Check",
        "Validation",
        "VatID",
        "Data",
        "Bank",
        "Transmission",
        "Austria",
        "File",
        "Upload",
        "Finanzamt"
    ],
    "authors": [
        {
            "name": "Christopher SÃ¶llinger",
            "email": "christopher.soellinger@gmail.com",
            "homepage": "https://github.com/CSoellinger"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-soap": "*",
        "jasny/typecast": "dev-master"
    },
    "require-dev": {
        "csoellinger/dog-html-printer": "^0.3",
        "klitsche/dog": "^0.4",
        "phpstan/phpstan": "^1.4",
        "ramsey/devtools": "^1.7",
        "symfony/dotenv": "^5.4",
        "symfony/process": "^5.4",
        "symfony/var-dumper": "^5.4",
        "vimeo/psalm": "^4.22"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CSoellinger\\FonWebservices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CSoellinger\\Test\\FonWebservices\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ramsey/composer-repl": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "captainhook/plugin-composer": true,
            "ramsey/devtools": true
        },
        "sort-packages": true
    },
    "extra": {
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        },
        "ramsey/devtools": {
            "analyze:phploc": {
                "script": "@dev:analyze:phploc"
            },
            "command-prefix": "dev",
            "report:phploc": {
                "script": "@dev:report:phploc"
            }
        }
    },
    "scripts": {
        "dev:analyze:phploc": "php ./bin/phploc.phar --count-tests src tests",
        "dev:report:phploc": "php ./bin/phploc.phar --count-tests --log-json phploc.json src tests"
    }
}
